<div class="card">
  <div class="profile">
    <img src="https://jackflacco.files.wordpress.com/2016/02/iron-man.jpg?w=900" class="avatar">
  </div>
  <section id="menu-container">
    <h1 class="title">{{username}}{{user.username}}{{userInDB.username}}</h1>
    <p class="subtitle">Email: {{email}}{{user.email}}{{userInDB.email}}</p>
    <p class="subtitle">SlackId: {{slackID}}{{user.slackID}}{{userInDB.slackID}}</p>
    <p class="subtitle">GoogleId: {{googleID}}{{user.googleID}}{{userInDB.googleID}}</p>
    <a href="/profile/edit">Edit profile</a>
  </section>
  <div id="formProfile">
    <button type="submit">Logout</button>
  </div>
</div>

